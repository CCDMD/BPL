DOCUMENT();

loadMacros(
  "PGstandard.pl",
  "MathObjects.pl",
  "PGcourse.pl", 
);


TEXT(&beginproblem);
$showPartialCorrectAnswers = 1;


$n = random(2,8,2);

Context()->variables->set(x=>{limits=>[0.1,1]});



BEGIN_TEXT

Dériver  \[y=x\arcsin(1-x^{$n})\]

$PAR

\(y' =\) \{ans_rule(40)\}

END_TEXT

$ans = Compute("arcsin(1-x^($n))- ($n*x^($n))/sqrt( x^($n)*(2-x^($n)))")->reduce;


ANS($ans->cmp);

ENDDOCUMENT();

} until($d2_value!=($k_1*$d1_value));

Context("ImplicitPlane");
Context()->noreduce('(-x)-y');
Context()->noreduce('(-x)+y');
$Plane1_1 = ImplicitPlane($P_1,$N1_1);
$Plane2_1 = ImplicitPlane($Q_1,$N2_1);


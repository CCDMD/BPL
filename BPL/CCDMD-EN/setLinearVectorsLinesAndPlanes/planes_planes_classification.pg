{
$N1_1 = Vector(non_zero_random(-9,9,1),non_zero_random(-9,9,1),non_zero_random(-9,9,1));
$k_1 = non_zero_random(-5,5,1);
$N2_1 = $k_1*$N1_1;

Context("Point");
$p1 = random(-9,9,1);
$p2 = non_zero_random(-9,9,1);
$p3 = random(-9,9,1);
$P_1 = Point($p1, $p2, $p3);

$Q_1 = Point($p1+1, $p2, $p3-1);

    $d1_value = $N1_1->extract(1)*$P_1->extract(1) + $N1_1->extract(2)*$P_1->extract(2) + $N1_1->extract(3)*$P_1->extract(3);
